<num> ::= 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9 | 0
<bool> ::= isChangingLane | isApproachingOnramp | isByGuardrail
<var> ::= V_vel | Lane_ID | NE_dist | N_dist | NW_dist | W_dist | SW_dist | S_dist | SE_dist | NE_vel | N_vel | NW_vel | W_vel | SW_vel | S_vel | SE_vel
<perc> ::= <digit> <digit> <digit> <digit>
