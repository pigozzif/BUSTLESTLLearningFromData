<expr> ::= <prop> | <logic> | <future1>
<prop> ::= <bool> | <var> <comp> <num>
<logic> ::= .not <expr> | .or <expr> <expr>
<future1> ::= .globally <expr2> <perc> <perc> | .eventually <expr2> <perc> <perc> | .until <expr2> <expr2> <perc> <perc>
<expr2> ::= <prop> | <logic2> | <future2>
<logic2> ::= .not <expr2> | .or <expr2> <expr2>
<future2> ::= .globally <special> <perc> <perc> | .eventually <special> <perc> <perc> | .until <special> <special> <perc> <perc>
<special> ::= <prop> | <logic3>
<logic3> ::= .not <special> | .or <special> <special>
<num> ::= <digit> <digit> <sign> <digit>
<comp> ::= < | > | <= | >= | ==
<digit> ::= 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9 | 0
<bool> ::= isChangingLane | isApproachingOnramp | isByGuardrail
<var> ::= V_vel | Lane_ID | NE_dist | N_dist | NW_dist | W_dist | SW_dist | S_dist | SE_dist | E_dist
<perc> ::= <digit> <digit>
<sign> ::= + | -
